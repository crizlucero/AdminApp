<?xml version="1.0" encoding="UTF-8" ?>
<selector xmlns:android="http://schemas.android.com/apk/res/android">
    <item android:state_checked="false" android:textColor="#000">
    <shape android:shape="rectangle">
            <corners android:radius="20dp" />
            <solid android:color="@color/switch_unselected" />
        </shape>
    </item>
    <item android:state_checked="true" android:textColor="#000" >
        <shape android:shape="rectangle">
            <corners android:radius="20dp" />
            <solid android:color="@color/switch_selected" />
        </shape>
    </item>
</selector>